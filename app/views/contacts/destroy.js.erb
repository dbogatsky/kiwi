$("#contact-button-grp-<%= @contact.id %>").find('a.tooltips').tooltip('hide');
$("#preloader").hide();
<% if @status.eql?("ok") %>
$("#contact-button-grp-<%= @contact.id %>").find('a.tooltips').tooltip('destroy');
$("#<%= dom_id(@contact) %>").remove();
jQuery.gritter.add({
	title: 'Success!',
	text: '<%= @notification_message %>',
	class_name: 'growl-success',
	image: '/assets/bootstrap/images/screen.png',
	sticky: false,
	time: ''
});
<% else %>
jQuery.gritter.add({
	title: 'Error!',
	text: '<%= @notification_message %>',
	class_name: 'growl-danger',
	image: '/assets/bootstrap/images/screen.png',
	sticky: false,
	time: ''
});
<% end %>