<!DOCTYPE html>
<html lang="en">
<head>
  <!-- Front-end Software Version: <%= APP_CONFIG['software_version'] %> -->

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
  <meta name="description" content="">
  <meta name="author" content="">
  <link rel="shortcut icon" href="" type="image/png">

  <title><%= APP_CONFIG['software_title'] %></title>
  
  <link href="assets/bootstrap/css/style.default.css" rel="stylesheet">
  <link href="assets/bootstrap/css/fullcalendar.css" rel="stylesheet">  
  <link rel="stylesheet" href="assets/vertical-timeline/css/style.css"> <!-- Resource style -->
  <script src="assets/vertical-timeline/js/modernizr.js"></script> <!-- Modernizr -->
  
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
  <script src="assets/bootstrap/js/html5shiv.js"></script>
  <script src="assets/bootstrap/js/respond.min.js"></script>
  <![endif]-->
</head>

<body>
<!-- Preloader -->
<div id="preloader">
    <div id="status"><i class="fa fa-spinner fa-spin"></i></div>
</div>

<section>
	<%= render "layouts/nav" %>
	<div class="mainpanel">
		<%= render "layouts/bar" %>  
		
		<div class="pageheader">
			<h2><i class="fa fa-home"></i> Dashboard</h2>
			<div class="breadcrumb-wrapper">
				<span class="label">stuff</span>
			</div>
		</div>
		
		<div class="contentpanel">
			<%= yield %>
		</div><!-- contentpanel -->
	
	</div><!-- mainpanel -->
  
</section>


<script src="assets/bootstrap/js/jquery-1.11.1.min.js"></script>
<script src="assets/bootstrap/js/jquery-migrate-1.2.1.min.js"></script>
<script src="assets/bootstrap/js/jquery-ui-1.10.3.min.js"></script>
<script src="assets/bootstrap/js/bootstrap.min.js"></script>
<script src="assets/bootstrap/js/modernizr.min.js"></script>
<script src="assets/bootstrap/js/jquery.sparkline.min.js"></script>
<script src="assets/bootstrap/js/toggles.min.js"></script>
<script src="assets/bootstrap/js/retina.min.js"></script>
<script src="assets/bootstrap/js/jquery.cookies.js"></script>

<script src="assets/bootstrap/js/flot/jquery.flot.min.js"></script>
<script src="assets/bootstrap/js/flot/jquery.flot.resize.min.js"></script>
<script src="assets/bootstrap/js/flot/jquery.flot.spline.min.js"></script>
<script src="assets/bootstrap/js/morris.min.js"></script>
<script src="assets/bootstrap/js/raphael-2.1.0.min.js"></script>

<script src="assets/bootstrap/js/custom.js"></script>
<script src="assets/bootstrap/js/dashboard.js"></script>

</body>
</html>

