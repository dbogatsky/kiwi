<% if assets_list.present? %>
  <% assets_list.each do |i| %>
      <%= render "assets_item_show.html.erb", :assets_item => i %>
    </div>
  <% end %>
<% else %>
  <div class="row">
    <div class="col-sm-12">
      <div class="row">
        <div class="col-md-12">
            <h3>No assets available</h3>
        </div>
      </div>
    </div>
  </div>
<% end %>

<%= render "assets_item_show_accordian.html.erb" %>

<% #render "assets_item_show.html.erb" %>

