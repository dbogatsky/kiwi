<%if @result%>
	var new_data = <%= raw(@notifiable_users) %>
  $("#notifiable_users").select2({
    data: new_data
  });
	$(".conversation-reminder-modal-form .select2").css('width', '100%');
	$("#render_upload_partial_for_reminder").html("<%=j render 'accounts/conversation_upload', box_for: 'reminder',account: @account%>");
  $('#preloader').hide();
<%else%>
  window.location.reload();
<%end%>
