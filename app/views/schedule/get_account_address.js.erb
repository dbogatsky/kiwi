$("#keyword").val('<%=@full_address%>')
<% if @array_of_hashes.present? %>
	$(".meeting_assign_to").html("<%= @account_assigned_to%>")
 	var array_of_hashes = []
 	<%@array_of_hashes.each do |a|%>
  	array_of_hashes.push({value: <%=a[:value]%>, text: '<%=a[:text]%>'})
 	<%end%>

	<%@array_of_hashes.each do |value|%>
	 	$(".meeting_assign_to").editable({
	                                  source: array_of_hashes,
	                                  value: "<%=value[:value]%>",
	                                  mode: 'inline',
	                                  inputclass: 'assign_to_select',
	                                  name: 'assign_to_select',
	                                  url: '/set_assign_to?account_id=<%=@account.id%>',
	                                  pk: '<%=value[:value]%>',
	                                  ajaxOptions: {
	                                    type: 'put'
	                                  },
	                                  success: function(response, newValue) {
	        if(response.status == 'error'){
	        	return response.msg;
	        }else{
	        	$(".assign_to_id").val(newValue);
	        }

	    },
	                                });
	<%end%>
<%end%>
$('#preloader').hide();