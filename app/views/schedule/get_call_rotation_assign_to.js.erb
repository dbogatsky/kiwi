<% if @assign_to_users_list.present? %>
  var assign_to_users_list = []
  <%@assign_to_users_list.each do |a|%>
    assign_to_users_list.push({value: <%=a[:value]%>, text: '<%=a[:text]%>'})
	<%end%>
  <%@assign_to_users_list.each do |value|%>
	 	$(".callrotation_assign_to").editable({
      source: assign_to_users_list,
      value: '<%=current_user.id%>',
      mode: 'inline',
      inputclass: 'assign_to_select',
      name: 'assign_to_select',
      url: '/schedule/set_assign_to?account_id=<%=@account.id%>',
      pk: '<%=value[:value]%>',
      ajaxOptions: {
        type: 'put'
      },
      success: function(response, newValue) {
        if(response == true){
          $(".callrotation_assign_to").val(newValue);
        }else{
          $(".callrotation_assign_to").val('')
        	return "user does not have access"
        }
      },
    });
  <%end%>
<%end%>
$('#preloader').hide();