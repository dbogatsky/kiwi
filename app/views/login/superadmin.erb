<body class="signin">
	
	
	<section>
	  
	    <div class="signinpanel">
	    
			<div class="row">
			
				<div class="col-xs-3">
				</div>
				
				<div class="col-md-7">
					<%= form_tag(login_superadmin_auth_path) do |f| %>

						<img src="/assets/images/code10.png" height="50" alt="Code10" />
						<br /><br />
						
						<p>Please select the company you wish to login as Admin for:</p>
						
						<select name="company" class="form-control input-sm">
			            	<option value="phone">ABC Company</option>
			            	<option value="phone">Acme Autoparts</option>
			            </select
						<br /><br />
						<input type="submit" class="btn btn-block btn-primary sharp" value="Continue">                 
					<% end %>
				</div>
				
				<div class="col-xs-3">
				</div>
		</div><!-- row -->
    
    </div><!-- signin -->
  
</section>


<script src="/assets/bootstrap/js/jquery-1.11.1.min.js"></script>
<script src="/assets/bootstrap/js/jquery-ui-1.10.3.min.js"></script>
<script src="/assets/bootstrap/js/jquery-migrate-1.2.1.min.js"></script>
